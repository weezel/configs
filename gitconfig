[user]
	name = Ville Valkonen
	email = example@mail.com
[color]
	ui = true
[core]
	excludesfile = $HOME/.gitignore
	autocrlf = input
	editor = gvim -f
[merge]
	tool = gvimdiff -f
[alias]
	co = checkout
	ci = commit
	st = status -bs
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	graph = log --graph --stat --pretty=oneline --abbrev-commit

