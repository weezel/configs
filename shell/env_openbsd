PATH=$HOME/bin:/bin:/sbin:/usr/bin:/usr/sbin:/usr/local/bin:/usr/local/sbin:/usr/X11R6/bin

LESSCHARSET=utf-8
LC_ALL=en_US.UTF-8
LANG=en_US.UTF-8

# Language specific settings
GO111MODULE=on
JAVA_HOME=/usr/lib/jvm/java-8-openjdk-amd64

case $TERM in
"alacritty" | "xterm")
	TERM="xterm-256color"
	;;
esac

export GO111MODULE HOME JAVA_HOME LANG LC_ALL LESSCHARSET PATH TERM

